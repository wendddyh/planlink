<%= simple_form_for @venue do |f| %>
  <%= f.input :timeslot, collection: [
      '11:00 AM', '11:30 AM',
      '12:00 PM', '12:30 PM',
      '01:00 PM', '01:30 PM',
      '02:00 PM', '02:30 PM',
      '03:00 PM', '03:30 PM',
      '04:00 PM', '04:30 PM',
      '05:00 PM', '05:30 PM',
      '06:00 PM', '06:30 PM',
      '07:00 PM', '07:30 PM',
      '08:00 PM', '08:30 PM'
    ]%>
  <%= f.button :submit, "Book", class: "btn btn-primary" %>
<% end %>
